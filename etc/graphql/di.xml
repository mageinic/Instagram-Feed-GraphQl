<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\StoreGraphQl\Model\Resolver\Store\StoreConfigDataProvider">
        <arguments>
            <argument name="extendedConfigData" xsi:type="array">
                <item name="instagramfeed_enable"
                      xsi:type="string">instagramfeed/general/enabled</item>
                <!--<item name="access_token"
                      xsi:type="string">instagramfeed/general/box_position</item>-->
                <item name="total_number"
                      xsi:type="string">instagramfeed/display/total_number</item>
                <item name="layout"
                      xsi:type="string">instagramfeed/display/layout</item>
                <item name="number_row"
                      xsi:type="string">instagramfeed/display/number_row</item>
                <item name="show_caption"
                      xsi:type="string">instagramfeed/display/show_caption</item>
                <item name="show_popup"
                      xsi:type="string">instagramfeed/display/show_popup</item>
            </argument>
        </arguments>
    </type>
</config>
